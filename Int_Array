//Create an Array of int type an store 10 elements in Array.
import java.util.Scanner;
class ArrayInt
{
    public static void main(String[] args)
    {    
       //datatype []variablename=new datatype[size];
            int array1[]= new int[10]; 

        //Creating a Scanner object for user input
             System.out.println("Enter the elements of Array:-");
             Scanner sc= new Scanner(System.in);

        // Taking user input for each Number in the array
        for (int i=0;i<array1.length;i++) 
        {
            array1[i] = sc.nextInt();
        }
       //Printing all the Number whatever we insert through user input
       for(int m=0;m<array1.length;m++)
       {
          System.out.println("The Numbers is:"+array1[m]);
       }

    }
}
